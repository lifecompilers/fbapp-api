  "bugs": {
    "url": "https://github.com/lifecompilers/fbapp-api/issues"
  },
  "homepage": "https://github.com/lifecompilers/fbapp-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}